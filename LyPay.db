BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "Users" (
	"id"	INTEGER UNIQUE,
	"email"	TEXT NOT NULL UNIQUE,
	"password"	TEXT,
	"socialMedia"	INTEGER NOT NULL UNIQUE,
	"personalDocs"	INTEGER NOT NULL UNIQUE,
	"paymentInfo"	INTEGER NOT NULL UNIQUE,
	"merchant"	INTEGER NOT NULL,
	PRIMARY KEY("id" AUTOINCREMENT)
);
COMMIT;
